{
    "manifest_version": 2,
    "name": "No Trackers",
    "version": "1.0",
    "description": "Skips trackers",
    "author": [{
        "name": "Jamezrin",
        "email": "jaime@jamezrin.name"
    }],
    "permissions": [
        "tabs",
        "activeTab",
        "webRequest",
        "webNavigation",
        "webRequestBlocking",
        "storage",
        "<all_urls>"
    ],

    "background": {
        "persistent": true,
        "scripts": ["main.js"]
    }
}